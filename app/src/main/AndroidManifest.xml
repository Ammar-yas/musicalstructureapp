<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ammar.musicalstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Music Library">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".nowPlaying"
            android:label="Now Playing"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".discoverMusic"
            android:label="Discover"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".buyMusic"
            android:label="buy Music"
            android:parentActivityName=".discoverMusic" />
    </application>


</manifest>